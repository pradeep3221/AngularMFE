{
  "name": ".",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:shell": "ng build shell",
    "build:mfe1": "ng build mfe1-dashboard",
    "build:mfe2": "ng build mfe2-user-management",
    "build:shared": "ng build shared",
    "watch": "ng build --watch --configuration development",
