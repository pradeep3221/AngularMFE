    "start:mfe2": "ng serve mfe2-user-management --port 4202",
    "serve:ssr:mfe1-dashboard": "node dist/mfe1-dashboard/server/server.mjs",
    "serve:ssr:mfe2-user-management": "node dist/mfe2-user-management/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
